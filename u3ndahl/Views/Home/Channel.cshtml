@*@model u3ndahl.Models.Program*@
@model List<u3ndahl.Models.Program>
@{
    ViewBag.Title = "Channel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Kanal</h2>

<div>
    <ul id="program">
        @foreach (var p in Model)
        {

            <li>@p.title @p.time @Html.ActionLink("Info", "ShowInfo")</li>

        }
    </ul>
</div>
