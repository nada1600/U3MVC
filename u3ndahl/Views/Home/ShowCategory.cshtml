@model List<u3ndahl.Models.Program>
@{
    ViewBag.Title = "Kategori";
}

<h2><strong>Kategori</strong></h2>
@*@Html.DisplayForModel("category")*@
@*@Html.DisplayNameForModel("category")*@
@*@Html.DisplayNameFor(model => model.Program.category);*@


<ul id="program">
    @foreach (var p in Model)
    {
        
        <li>@p.category| @p.channel || @p.date | @p.title @Html.ActionLink("(Info)", "ShowInfo", "Home", new { id = p.id }, null)</li>
    }

</ul>





<div id="back">
    
    @Html.ActionLink("Tillbaka", "Index")
</div>
